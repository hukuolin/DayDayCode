<?xml version="1.0" encoding="UTF-8"?>
<abroad_student>
  <operation>
      <compowers>
        {compowers}
        <!--全权用户身份证号，非空-->
      </compowers>
  </operation>
  <pilot>
      <!--基本信息-->
      <indentitytype>
      {indentitytype}
        <!--身份证明类型，非空-->
      </indentitytype>
      <identifycode>{identifycode}
        <!--身份证明编号，非空-->
      </identifycode>
      <pilotname>{pilotname}
        <!--中文名，中国国籍非空-->
      </pilotname>
      <pilotnameeng>{pilotnameeng}
        <!--英文名，非空-->
      </pilotnameeng>
      <nationality>{nationality}
        <!--国籍，非空-->
      </nationality>
      <birthplace>{birthplace}
        <!--出生地，非空-->
      </birthplace>
      <birthday>{birthday}
        <!--出生日期，非空-->
      </birthday>
      <sex>{sex}
        <!--性别，非空-->
      </sex>
      <nation>{nation}
        <!--民族，非空-->
      </nation>
      <mobile>{mobile}
        <!--手机号，非空，合法-->
      </mobile>
      <manager>{manager}
        <!--管理局，非空-->
      </manager>
      <org>{org}
        <!--单位，非空-->
      </org>
      <address>{address}
        <!--永久地址，非空-->
      </address>
      <zipcode>{zipcode}
        <!--邮编，非空-->
      </zipcode>
      <mailaddr>{mailaddr}
        <!--通信地址，非空-->
      </mailaddr>
      <xueli>{xueli}
      <!--中国国籍必填，外籍可选。（本科/硕士研究生/博士研究生/高中/大专/初中/中专）-->
      </xueli>
  </pilot>
  <base>
      <school_id>{school_id}</school_id><!--[外送学校编号]-->
      <register_date>{register_date}</register_date><!--[送出日期]-->
      <isgraduaction>{isgraduaction}</isgraduaction><!--[外送注册状态：0.在训;1.停训；4.毕业]-->
  </base>
</abroad_student>